<mvc:View controllerName="com.zhrxchangeshiftapp.zhrxchangeshiftapp.controller.AppChgList" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" height="100%">
    <Page id="page" title="List Document ID">
        <subHeader>
            <OverflowToolbar id="_IDGenOverflowToolbar1">
                <SearchField id="_IDGenSearchField1" liveChange=".onSearch" width="100%" />
            </OverflowToolbar>
        </subHeader>
        <List id="idList" items="{/zhr_ecshift_hSet}" selectionChange=".onSelectionChange" mode="SingleSelectMaster" growing="true" growingThreshold="50" includeItemInSelection="true">
            <infoToolbar>
                <OverflowToolbar visible="false" id="idInfoToolbar">
                    <Label id="idFilterLabel" />
                </OverflowToolbar>
            </infoToolbar>
            <StandardListItem id="_IDGenStandardListItem1" title="{Zzdocid}" description="{Zzorgeh}" iconDensityAware="false" iconInset="false" />
        </List>
    </Page>
</mvc:View>
